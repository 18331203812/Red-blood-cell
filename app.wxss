@import "./utils/weui.wxss";
@import "./utils/main.wxss";
page{
   font-size: 32rpx;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
view{
  box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 搜索icon */
.search-icon{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center / 30rpx auto;
}
/* 热 icon */
.heat-icon{
  /* width: 47rpx; */
  /* height: 25rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: #ce131b;
  border: 1rpx solid #ce131b;
  border-radius: 5rpx;
  padding: 0 10rpx;
}
/* 查看 */
.see-icon{
  width: 38rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 38rpx auto;
  margin:  0 10rpx;
}
/* 评论首页 */
.message-icon{
  width: 34rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 34rpx auto;
  margin:  0 10rpx;
}
/* 评论详情 */
.comment-icon{
  width: 32rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 32rpx auto;
  margin: 0 10rpx;
}
/* 未点赞 */
.fabulou-icon{
  width: 32rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 32rpx auto;
  margin:  0 10rpx;
}
/* 未点赞（评论） */
.fa-icon{
  width: 32rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 32rpx auto;
  margin:  0 10rpx;
}
/* 点赞成功 */
.fabulou-icons{
  height: 100%;
  width: 32rpx;
  background: url(data:image/png;base64,) no-repeat center / 32rpx auto;
   margin:  0 10rpx;
}
/* 分享 */
.share-icon{
  width: 27rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 27rpx auto;
  margin:  0 10rpx;
}
/* 时间 */
.time-icon{
  width: 28rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 28rpx auto ;
  margin: 0 10rpx;
}
/* 地点 */
.address-icon{
  width: 27rpx;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 27rpx auto ;
  margin: 0 10rpx;
}
button{
  margin: 0;
  padding: 0;
  outline: none;
  border: none;
}
button::after{
   border: none;
}

/* 积分背景页 */

.nav-integral{
  width: 100%;
  height: 320rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  position: relative;
}
.more-icon{
  width: 14rpx;
  height: 22rpx;
  background: url(data:image/png;base64,) no-repeat center / 14rpx 22rpx;
}