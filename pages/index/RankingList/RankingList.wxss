.nav{
  width: 100%;
  height: 382rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% 101%;
  position: relative;
}
.nav-tabBar{
  display: flex;
  justify-content: space-around;
  padding: 32rpx 0 20rpx 0;
}
.nav-tabBar-li{
  font-size: 28rpx;
  color: #ffffff;
}
.ranking-box{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0 0 0;
  position: relative;
}
.ranking-box-li{
  width: 233rpx;
  border-radius: 20rpx;
  box-shadow: 5rpx 0 8rpx rgba(112, 17, 21, 0.3);
  padding: 30rpx 0;
  background: #fff;
}
.ranking-box-position{
  position: absolute;left: 0;right: 0;margin: 0 auto;
  top: -14rpx;
  width: 250rpx;
  padding: 30rpx 0 45rpx 0;
}
.ranking-box-li-title{
  font-size: 30rpx;
  color: #333333;
  text-align: center;
  padding: 30rpx 0  10rpx 0;
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
  font-weight: bold;
}
.ranking-box-li-text{
  font-size: 24rpx;
  color: #666666;
  text-align: center;
}
.ranking{
  padding: 0 30rpx 20rpx;
  width: 100%;
}
.ranking-head{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ranking-name{
  font-size: 30rpx;
  color: #333333;
}
.ranking-more{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #999999;
}
.ranking-li-img{
  display: flex;
  justify-content: center;
  position: relative;
}
.ranking-li-img-head{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  border: 4rpx solid #d42f36;
}
.ranking-li-img-footer{
  width: 100rpx;
  height: 30rpx;
  position: absolute;
  bottom: -10rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}
.nav-footer{
  width: 100%;
  height: 40rpx;
  background: linear-gradient(to right,#fa4945,#c11219);
  position: absolute;
  bottom: -38rpx;
  right: 0;
}
.main{
  display: flex;
  justify-content: center;
}
.main-box{
  width: 80%;
  border-radius: 0 0 10rpx 10rpx;
  box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.1);
  padding: 0 30rpx 30rpx;
}
.box-li{
  display: flex;
  align-items: center;
  padding: 30rpx  30rpx 0 30rpx;
}
.box-num{
  font-size: 36rpx;
  color: #333333;
  padding-right: 20rpx;
}
.box-con-img{
  display: block;
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
}
.box-con{
  display: flex;
  align-items: center;
}
.box-name{
  font-size: 28rpx;
  color: #666666;
}
.box-head{
  padding-left: 20rpx;
}
.box-text{
  color: #333333;
  font-size: 32rpx;
}
.main{
  padding-top: 40rpx;
}
.category{
  font-size: 34rpx;
  font-weight: bold;
}