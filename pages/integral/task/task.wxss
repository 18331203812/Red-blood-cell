.nav-text{
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  padding-top: 30rpx;
}
.nav-number{
  text-align: center;
  font-size: 100rpx;
  color: #fff;
}
.nav-but{
  display: flex;
  justify-content: space-around;
  padding:  0 140rpx;
}
.nav-box{
  width: 92%;
  border-radius: 10rpx;
  box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.2);
  background: #fff;
  display: inherit;
  padding: 30rpx 20rpx;
  margin: 0 auto;
}
.box-title{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  color: #ce131b;
  font-weight: bold;
}
.box-title-icon-left{
  width: 20rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  margin: 0 12rpx;
}
.box-title-icon-right{
  width: 20rpx ;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
   margin: 0 10rpx;
}
.box-text{
  text-align: center;
  font-size: 24rpx;
  color: #666666;
  padding: 10rpx 0 40rpx 0;
}
.box-ul{
  display: flex;
  width: 100%;
}
.box-li{
  width: calc(100% / 7);
}
.li-radius{
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  border: 1rpx solid #666666;
  text-align: center;
  line-height: 68rpx;
  font-size: 24rpx;
  display: inherit;
  margin: 0 auto;
  color: #666666;
  position: relative;
}
.li-text{
  text-align: center;
  font-size: 24rpx;
  color: #666666;
  padding: 25rpx 0 40rpx 0;
}
.box-but{
  width: 342rpx;
  height: 68rpx;
  background: linear-gradient(to right ,#f92733,#cf131c);
  line-height: 68rpx;
  box-shadow: 0 0 20rpx rgba(206, 19, 27, 0.42);
  font-size: 32rpx;
  color: #fff;
  font-weight: bold;
  border-radius: 68rpx;
  display: inherit;
  margin: 0 auto;
}
.category{
  background: linear-gradient(to right ,#f2242f,#d4151f);
  border: none;
  color: #fff;
}
.li-icon{
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}
.li-icons{
  width: 100%;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
}
.border{
  border: 1rpx solid #ca131a;
}
.main-title{
  padding: 26rpx 20rpx 0 20rpx;
  position: relative;
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
}
.main-title::after{
  content: '';
  width: 8rpx;
  height: 32rpx;
  background: #ce131b;
  border-radius: 4rpx;
  position: absolute;
  left: 0;
  top: 20rpx;
  bottom: 0;
  margin: auto 0;
}
.main{
  width: 100%;
  padding: 300rpx 30rpx 30rpx 30rpx;
}
.main-text{
  padding: 10rpx 40rpx;
  border-bottom: 1rpx solid #e3e3e3;
  font-size: 24rpx;
  color: #999999;
}
.main-li{
  padding: 30rpx 30rpx;
  border-bottom: 1rpx solid #e3e3e3;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.main-li-name{
  display: flex;
  align-items: center;
  font-size: 32rpx;
  color: #333333;
}
.main-li-img{
  max-width: 34rpx;
  max-height: 34rpx;
  display: block;
  margin-right: 20rpx;
}
.main-li text{
  font-size: 24rpx;
  color: #999999;
}
.borders{
  border: none
}