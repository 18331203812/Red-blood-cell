.nav{
  padding: 80rpx 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.nav-status{
  font-size: 24rpx;
  color: #fff;
  padding: 0 10rpx;
  height: 40rpx;
  background: #ce131b;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5rpx;
  width: 77rpx;
}
.nav-img{
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.3);
  margin-right: 24rpx;
}
.head{
  width: 132rpx;
  height: 132rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  position: relative;
}
.nav-imgs{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  position: absolute;
  bottom: 16rpx;
  right: 23rpx;
}
.nav-con{
  display: flex;
  align-items: center;
  font-size: 48rpx;
  color: #333333;
  width: 100%;
  justify-content: space-between;
}
.grant{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #333333;
}
.more-icon{
  margin-left: 20rpx;
}
.main{
  padding: 0 30rpx;
}
.main-li{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #333333;
  padding: 30rpx 0;
  font-weight: bold;
}
.go{
  display: flex;
  font-weight: 0;
  font-size: 24rpx;
  color: #ce131b;
  align-items: center;
}
.nav-con-img{
  width: 27rpx;
  height: 27rpx;
  display: block;
  margin-right: 10rpx;
}
.nav-cons{
  display: flex;
  align-items: center;
}
