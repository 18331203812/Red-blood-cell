.cur{
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box{
  width: 472rpx;
  /* height: 357rpx; */
  background: #fff;
  border-radius: 20rpx;
  position: relative;
  padding-top: 207rpx;
}
.box-img{
  max-width: 351rpx;
  max-height: 250rpx;
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  margin:  0 auto;
  top: -30rpx;
}
.box-detele{
  width: 52rpx;
  height: 65rpx;
  background: url(data:image/png;base64,) no-repeat center / 100% auto;
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 auto ;
  bottom: -60rpx;
}
.title{
  text-align: center;
  font-size: 32rpx;
  color: #333333;
}
.text{
  text-align: center;
  font-size: 26rpx;
  color: #999999;
}
.but{
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
}
.cancel{
  width: 200rpx;
  height: 65rpx;
  border-radius: 32rpx;
  border: 1rpx solid #ce131b;
  font-size: 32rpx;
  color: #ce131b;
  display: flex;
  justify-content: center;
  background: #fff;
  align-items: center;
}
.immediately{
    width: 200rpx;
  height: 65rpx;
  border-radius: 32rpx;
  background: #ce131b;
  font-size: 32rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.actiy{
  width: 337rpx;
}